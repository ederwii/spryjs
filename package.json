{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "express",
    "spryjs",
    "rapid"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --runInBand --detectOpenHandles --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ederwii/spryjs.git"
  },
  "bugs": {
    "url": "https://github.com/ederwii/spryjs/issues"
  },
  "homepage": "https://github.com/ederwii/spryjs#readme",
  "name": "spry-js",
  "author": "Alan Martinez",
  "module": "dist/spry.esm.js",
  "size-limit": [
    {
      "path": "dist/spry.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/spry.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/supertest": "^2.0.11",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "size-limit": "^4.11.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0"
  }
}
